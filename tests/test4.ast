(Program
	(PROC_DECL (TYPE float) (NAME Sum)
		PARAM_DECL (TYPE float) (NAME v1))
		PARAM_DECL (TYPE float) (NAME v2))

		(RETURN_STMT (ARIT_OP +
			(NAME v2)
			(NAME v1)
		))
	)
	(PROC_DECL (TYPE null) (NAME Main)
		(PROC_DECL (TYPE int) (NAME InnerSum)
			PARAM_DECL (TYPE int) (NAME v1))
			PARAM_DECL (TYPE int) (NAME v2))

			(RETURN_STMT (ARIT_OP +
				(NAME v2)
				(NAME v1)
			))
		)
		(VAR_DECL (TYPE float) (NAME f))
		(VAR_DECL (TYPE int) (NAME v))

		(ASSIGN STMT
