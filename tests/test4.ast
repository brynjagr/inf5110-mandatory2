				(ACTUAL_PARAM (FLOAT_LITERAL 41.5))
				(ACTUAL_PARAM (FLOAT_LITERAL 0.5))
			)
		)
		(ASSIGN STMT
			(NAME v)
			(CALL_STMT (NAME InnerSum)
				(ACTUAL_PARAM (INT_LITERAL 40))
				(ACTUAL_PARAM (INT_LITERAL 2))
			)
		)
		(CALL_STMT (NAME printfloat)
			(ACTUAL_PARAM (NAME f))
		)
